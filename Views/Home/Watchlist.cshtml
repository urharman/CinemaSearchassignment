@{
    ViewData["Title"] = @ViewData["Title"];
}

<form action="/Home">
    <input type="submit" value="Home" />
</form>

@{int i = 0;}
    @foreach (var movies in ViewBag.display)
    {

        <table border="0" cellpadding="0" cellspacing="0" style="width:25%;float:left">
            <tbody>

                @foreach (var item in movies)
                {
                    <tr>
                        <td>@item</td>
                    </tr>
                    i++;
                }
            </tbody>
        </table>

    }